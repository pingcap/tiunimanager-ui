import i18next from 'i18next'

const addMessages = () => {
  i18next.addResourceBundle('en', 'error', {
    {{- range . }}
    '{{ .Code }}': `{{ .En }}`,
    {{- end }}
  })
  i18next.addResourceBundle('zh', 'error', {
    {{- range . }}
    '{{ .Code }}': `{{ .Zh }}`,
    {{- end }}
  })
}

if (i18next.isInitialized) addMessages()
else i18next.on('initialized', addMessages)